{% extends "::layout_admin.html.twig" %}
{% form_theme form 'DomaineBundle:Form:fields.html.twig' %}

{% block section %}


<h1>Créer un nouveau menu pour l'activité : {{ categorie.nom }} </h1>
<div class="well">
  <form action="{{path('domaine_admin_ajouter_menu_activite', {'idActivite' : categorie.id })}} " method="post" {{ form_enctype(form) }}>
  	<table class="table responsive">
  		<tbody>
  			{{ form_widget(form) }}
  		</tbody>
  	</table>
   
    <input type="submit" class="btn btn-success" />
  </form>
</div>

{% endblock %}