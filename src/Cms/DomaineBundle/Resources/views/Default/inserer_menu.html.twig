{% for menu in menus %}
    {% if menu.pages is defined %}
      {% if menu.pages is not empty %}

        {% if menu.pages|length == 1 %}
          <h1>Site internet en construction</h1>


        {% elseif menu.pages|length == 2 %}
          {% for page in menu.pages %}
            <div class="col-lg-6">
              <div id="myCarousel{{ page.id }}" class="carousel slide" data-ride="carousel">
              <!-- Indicators -->
              <ol class="carousel-indicators">
                <li data-target="#myCarousel{{ page.id }}" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel{{ page.id }}" data-slide-to="1"></li>
                <li data-target="#myCarousel{{ page.id }}" data-slide-to="2"></li>
              </ol>
              
              <div class="carousel-inner" role="listbox">
              {% if page.photos is empty %}
                <div class="item active">
                  <div class="container">
                    <div class="carousel-caption">
                      <h1> {{ page.nom }} </h1>
                      <p></p>
                      <p><a class="btn btn-lg btn-primary" href="#" target="_blank" role="button">En construction</a></p>
                    </div>
                  </div>
                </div>
              {% endif %}
              {% if page.photos is not empty %}
                <div class="item active">
                  <img src="{{asset(page.photos['0'].webPath)}} " alt="ball trap du rabot" title="evenement le rabot balltrap">
                  <div class="container">
                    <div class="carousel-caption">
                      <h1> {{ page.nom }} </h1>
                      <p></p>
                      <p>
                        {% if page.url is defined %}
                        <a class="btn btn-lg btn-primary" href="{{ page.url }}" target="_blank" role="button">Visiter</a>
                        {% else %}
                        <a class="btn btn-lg btn-primary" href="{{path('domaine_voir_page', {'idpage': page.id })}} " target="_blank" role="button">Visiter</a>
                        {% endif %}

                      </p>
                    </div>
                  </div>
