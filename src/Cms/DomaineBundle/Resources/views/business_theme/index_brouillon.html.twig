                    </div>
                    <div class="fourth-arrow hidden-xs">
                        <hr> <i class="fa fa-angle-down"></i>
                    </div>
                </div> <!--skill_border-->       
            </div><!--section-->
    {% endif %}
{% endblock %}

{% block partenaires %}
    {% if partenaires is not empty %}
    <section id="partner">
        <div class="container">
            <div class="center wow fadeInDown">
                <h2>Our Partners</h2>
                <p class="lead">Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut <br> et dolore magna aliqua. Ut enim ad minim veniam</p>
            </div>    

            <div class="partners">
                <ul>
                    {% for partenaire in partenaires %}
                    <li>
                        <a href="#">
                        {% if partenaire.photo is not empty %}
                           <img class="img-responsive wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="300ms" src="{{asset(partenaire.photo.webPath)}} ">
                           {% endif %}
                        </a>
                    </li>
                    {% endfor %}

                </ul>
            </div>        
        </div><!--/.container-->
    </section><!--/#partner-->
    {% endif %}
{% endblock %}
