{% extends "::layout_admin.html.twig" %}

{% form_theme form 'DomaineBundle:Form:fields.html.twig' %}


{% block section %}
<h1>Modification de la page :</h1>
<div class="well">
  <form action="#" method="post" {{ form_enctype(form) }}>
  	<table class="table responsive">
  		{{ form_widget(form) }}
