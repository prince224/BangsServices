{% extends "::layout_admin.html.twig" %}

{% form_theme form 'DomaineBundle:Form:fields.html.twig' %}


{% block section %}
<h1>Ajout d'une section pour le sous-menu {{ sousmenu.nom|upper }} :  page <strong>{{ sousmenu.page.nom }} </strong></h1>
<div class="well">
  <form action="#" method="post" {{ form_enctype(form) }}>
  	<table class="table responsive">
  		<tbody>
  			{{ form(form) }}
  		</tbody>
  	</table>
   
    <input type="submit" class="btn btn-success" />
  </form>
</div>
{% endblock %}
